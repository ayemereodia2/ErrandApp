{
  "expo": {
    "name": "Swave",
    "slug": "errand-app-by-swave",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/new-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/app-1.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
      "plugins": [ 
         "@react-native-firebase/app",
          [
          "expo-build-properties",
          {
            "ios": {
              "useFrameworks": "static"
            }
          }
        ],
        [
          "expo-font",
          {
            "fonts": ["path/to/file.ttf"]
          }
        ],
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you share them with your friends."
          }
        ],
        [
          "expo-location",
          {
            "locationAlwaysAndWhenInUsePermission": "Allow Swave to use your location."
          }
        ]
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/*"
    ],
    "notification": {
      "icon": "./assets/images/icon-bg.png",
      "color":"#a9c1f8"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber":"17.0",
      "config": {
        "googleMapsApiKey": "AIzaSyDHfdBmUpWupA3f4Ld0lNTuQbbJQGJ4CSo"
      },
      "bundleIdentifier": "com.orj5.Swave",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-bg.png",
        "backgroundColor": "#a9c1f8"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD08WNri8gRUo8FizodlORIFhHyrftu3nw"
        }
      },
      "package": "com.orj5.Swave",
      "versionCode": 11
    }, 
     "plugins": [
      "@react-native-firebase/app"
    ],
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b7506e62-37fe-4929-b6c8-dde1a38b9139"
      }
    }
  }
}
